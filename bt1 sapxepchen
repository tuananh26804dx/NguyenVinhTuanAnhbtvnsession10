#include <stdio.h>

int main(){
    int arr[] = {1,4,2,5,2,3,5,6,2,5,6};
    int n = sizeof(arr)/sizeof(arr[0]);
    int k, temp;

  
    for(int i = 1; i < n; i++){
        k = 0;
        
        while(k < i && arr[k] < arr[i]){  
            k++;
        }
        
        // while(k < i && arr[k] > arr[i])  // 
        temp = arr[i];
        for(int j = i; j > k; j--){
            arr[j] = arr[j-1];
        }
        arr[k] = temp;
    }

    printf("\nMang sap xep tang dan bang thuat toan chen:\n");
    for(int i = 0; i < n; i++){
        printf("%d  ", arr[i]);
    }

    return 0;
}
